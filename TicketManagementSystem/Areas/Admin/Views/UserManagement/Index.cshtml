
@model IEnumerable<TicketManagementSystem.Models.User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>Add Users : <a href="@Url.Action("Create", "UserManagement")">Add User</a></div>

<table class="table">
    <thead>
        <td>Name</td>
        <td>Role</td>
        <td>Actions</td>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Role</td>
            <td>
                <a href="@Url.Action("#","#")">Edit</a>
                <br />
                <a href="@Url.Action("#","#")">Delete</a>
            </td>
        </tr>
    }
</table>
